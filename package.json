{
  "name": "bearicorn",
  "version": "2.0.5",
  "description": "Cli for Bearicorn",
  "main": "dist/bear.js",
  "preferGlobal": true,
  "license": "Apache-2.0",
  "homepage": "https://github.com/ondrek/bearicorn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ondrek/cli.git"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "______xx": "babel bear.js -w src/utils -w options -w templates --out-file compiled.js --presets=@babel/preset-env --verbose",
    "watchxxxwatchxxx": "nodemon --exec babel-node src/bear.js",
    "prepublish": "babel src --out-dir dist",
    "watch": "babel src --watch --out-dir dist --verbose",
    "serve": "node dist/bear.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "npm version patch && npm publish"
  },
  "bin": {
    "bearicorn": "dist/bear.js"
  },
  "dependencies": {
    "arg": "4.1.3",
    "chalk": "3.0.0",
    "dotenv": "8.2.0",
    "form-data": "3.0.0",
    "inquirer": "7.1.0",
    "js-yaml": "3.13.1",
    "jsonwebtoken": "8.5.1",
    "listr": "0.14.3",
    "node-fetch": "2.6.0",
    "prompts": "2.3.2",
    "tar": "6.0.1",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/runtime": "7.9.2",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.0"
  }
}
