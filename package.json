{
  "name": "bear",
  "version": "2.0.18",
  "description": "Deploy any language from your command line",
  "main": "dist/bear.js",
  "preferGlobal": true,
  "license": "Apache-2.0",
  "homepage": "https://github.com/ondrek/bear",
  "repository": {
    "type": "git",
    "url": "https://github.com/ondrek/bear"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "DEBUG=eslint:cli-engine ./node_modules/.bin/eslint ./src",
    "jest": "jest tests",
    "prepublish": "babel src --out-dir dist",
    "watch": "babel src --watch --out-dir dist --verbose",
    "serve": "node dist/bear.js",
    "upload": "npm version patch && npm publish"
  },
  "bin": {
    "bear": "dist/bear.js"
  },
  "keywords": [
    "hosting",
    "docker",
    "aws",
    "gc",
    "deploy",
    "cli",
    "cheap",
    "easy",
    "platform",
    "unicorn"
  ],
  "dependencies": {
    "archiver": "^3.1.1",
    "arg": "4.1.3",
    "chalk": "3.0.0",
    "cli-cursor": "3.1.0",
    "form-data": "3.0.0",
    "inquirer": "7.1.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "8.5.1",
    "log-symbols": "3.0.0",
    "node-fetch": "2.6.0",
    "ora": "4.0.3",
    "promptly": "3.0.3",
    "tar": "6.1.9",
    "tar-fs": "^2.0.1",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.0",
    "@babel/runtime": "7.9.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.2.7"
  }
}
